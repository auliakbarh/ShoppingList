{
  "name": "shoppinglist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "install-release:android": "npx react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "code-push-production:ios": "appcenter codepush release-react -a auliakbarh/Shopping-List -d Production",
    "code-push-staging:ios": "appcenter codepush release-react -a auliakbarh/Shopping-List -d Staging",
    "code-push-production:android": "appcenter codepush release-react -a auliakbarh/Shopping-List-1 -d Production",
    "code-push-staging:android": "appcenter codepush release-react -a auliakbarh/Shopping-List-1 -d Staging"
  },
  "dependencies": {
    "appcenter": "4.3.0",
    "appcenter-analytics": "4.3.0",
    "appcenter-crashes": "4.3.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.5",
    "tailwind-react-native-classnames": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
